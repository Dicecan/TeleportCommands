{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://mrsnowy.dev/",
        "sources": "https://github.com/MrSn0wy/TeleportCommands",
        "issues": "https://github.com/MrSn0wy/TeleportCommands/issues"
    },
    "license": "${license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "${group}.fabricInit"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json"
    ],
    "custom": {
        "modmenu": {
            "links": {
                "modmenu.modrinth": "https://modrinth.com/mod/teleport-commands",
                "modmenu.kofi": "https://ko-fi.com/mr_snowy"
            }
        }
    },
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "minecraft": "~${minecraft_version}",
        "java": ">=${java_version}"
    }
}
  